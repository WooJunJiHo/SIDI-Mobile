{
  "expo": {
    "name": "Mobile",
    "slug": "Mobile",
    "version": "1.0.0",
    "platforms": ["ios", "android"],
    "scheme": "SIDI-Mobile",
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icons/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.SIDI.Mobile",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["com.googleusercontent.apps.733666115774-37prtfvmin9u4cioh1dg38upvjuijblv"]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.Mobile"
    },
    "web": {
      "favicon": "./assets/icons/icon.png"
    },
    "expo": {
      "plugins": [
        [
          "expo-barcode-scanner",
          {
            "cameraPermission": "Allow $(PRODUCT_NAME) to access camera."
          }
        ]
      ]
    },
    "extra": {
      "eas": {
        "projectId": "864f402d-42b5-4c17-ac72-6408c8aae4f3"
      }
    }
  }
}
